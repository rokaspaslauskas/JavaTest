import java.util.ArrayList;

public class Koordinate {

	private int y;
	private int x;
	
	
	
	
	public Koordinate(int y2, int x2) {
		this.y = y;
		this.x = x;
	}
	
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	
	public String toString() {
		
		return getX()+ " "+ getY();
	}
	
	
}
