
public class antraUz {

	public static void main(String[] args) {
				spausdink("Rokas","Paslauskas","JAVA", 22);
				
	}
		public static void antraUz.spausdink(String vardas; String pavarde;
				String kalba; int savaiciuSkaicius) {
			System.out.println("Að, "+[vardas] +" "+[pavarde]+
					", tikrai iðmoksiu programuoti "+[kalba]+" kalba per"
					+ [savaiciuSkaicius]+" savaites");
		}
	
	
	
	
	
	
	
	
	
	
	
	
}
